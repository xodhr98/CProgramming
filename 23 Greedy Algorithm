#include <iostream>

int main() {
	std::cout << "Greedy Algorithm\n";
	int num = 1260;
	int result = 0;
	result += num / 500;
	num = num % 500;

	result += num / 100;
	num = num % 100;

	result += num / 50;
	num = num % 50;

	result += num / 10;
	num = num % 10;
	std::cout << "result: " << result;
}
